import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		//your code here
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int r,sum=0;
    int temp=num;
    while(num>0)
    {
      r=num%10;    
      sum=(sum*10)+r;    
      num=num/10; 
    }
    if(temp==sum)
    {
      System.out.println("true");
    }
    else
    {
      System.out.println("false");
    }
	}
}
